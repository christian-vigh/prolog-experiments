

:- op( 500, xfx, covers ).

:- op(1200,  fx, #<).
:- op(1200,  fx, #>).
:- op(1200,  xf, <#).
:- op(1200,  xf, >#).
:- op( 700, xfx, --).
:- op( 800, xfx, and ).


X covers A :-
	write(A).
	

interval(Left, Low, High, Right) :-
	write(Left), nl,
	write(Low), nl,
	write(High), nl,
	write(Right), nl.