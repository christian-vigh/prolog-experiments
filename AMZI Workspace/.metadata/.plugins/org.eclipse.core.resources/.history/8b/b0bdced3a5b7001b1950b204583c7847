% Fact base
concept(reality).
	belongs(reality, air).
		attribute(air, breathe).
		
	belongs(reality, animals).
		belongs(animals, dogs).
			attribute(dogs, species).
			attribute(dogs, eyes, 2).
			attribute(dogs, legs, 4).

			action(dogs, bark).
			action(dogs, sleep).
			action(dogs, eat).
			action(dogs, drink).
			action(dogs, bite).
			action(dogs, run).
		
			belongs(dogs, yorkshire).
			belongs(dogs, setter).

		belongs(animals, birds).
			attribute(birds, fly).
			attribute(birds, sing).

